{
    "env": {
        "es2022": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "off"
    },
    "plugins": ["prettier"],
    "root": true
}
